{
  "name": "ddex-parser",
  "version": "0.4.1",
  "description": "DDEX XML parser with native and WASM bindings",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node",
    "README.md",
    "LICENSE"
  ],
  "napi": {
    "name": "ddex-parser-node",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "arm-linux-androideabi",
        "armv7-unknown-linux-gnueabihf"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "build:ts": "tsc",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "version": "napi version",
    "test": "node test-manual.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ddex",
    "xml",
    "parser",
    "music",
    "metadata",
    "ern",
    "native",
    "rust",
    "wasm"
  ],
  "author": "Kevin Marques Moo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daddykev/ddex-suite.git",
    "directory": "packages/ddex-parser"
  },
  "homepage": "https://github.com/daddykev/ddex-suite/tree/main/packages/ddex-parser",
  "bugs": {
    "url": "https://github.com/daddykev/ddex-suite/issues"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@types/node": "^20.19.13",
    "typescript": "^5.9.2"
  },
  "optionalDependencies": {
    "ddex-parser-win32-x64-msvc": "0.4.1",
    "ddex-parser-darwin-x64": "0.4.1",
    "ddex-parser-linux-x64-gnu": "0.4.1",
    "ddex-parser-linux-x64-musl": "0.4.1",
    "ddex-parser-linux-arm64-gnu": "0.4.1",
    "ddex-parser-darwin-arm64": "0.4.1",
    "ddex-parser-android-arm64": "0.4.1",
    "ddex-parser-android-arm-eabi": "0.4.1",
    "ddex-parser-linux-arm-gnueabihf": "0.4.1"
  }
}